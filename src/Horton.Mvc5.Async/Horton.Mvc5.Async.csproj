<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Description>Horton MVC5 Async Extensions</Description>
        <AssemblyTitle>Horton MVC5 Async Extensions</AssemblyTitle>
        <AssemblyName>Horton.Mvc5.Async</AssemblyName>
        <PackageId>Horton.Mvc5.Async</PackageId>
        <VersionPrefix>1.0.0</VersionPrefix>
        <VersionSuffix>alpha</VersionSuffix>
        <Copyright>2017 Joseph Daigle</Copyright>
        <TargetFramework>net452</TargetFramework>
        <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
